<div class="container">
  {{!-- Display the page title. --}}
  <div class="row" id="first-row">
    <div align="center" class="col-md-12">
      <h1>News Scrape</h1>
      <h3>From Streetsblog San Francisco</h3>
    </div>
  </div>
  {{!-- Use the burger-block.handlebars file to display the list of not devoured burgers, the list of devoured burgers, and the Devour It button for each not devoured burger. --}}
  <div class="row" id="second-row">
    <div class="col-md-12">
    <ol>
      {{#each articles}}
       {{!--  {{#unless devoured}} --}}
          {{!-- {{> article-block}} --}}
        <li>
          Title: {{title}}<br>
          Link: {{link}}<br>
          Summary: {{summary}}<br><br>
        </li>
       {{!--  {{/unless}} --}}
      {{/each}}
    </ol>
    </div>
